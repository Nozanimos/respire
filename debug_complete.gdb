# FONCTION: Afficher tous les widgets de la liste
# â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
define show_all_widgets
    set $list = $arg0
    set $node = $list->first
    set $idx = 0

    while ($node != 0)
        show_widget $node $idx
        set $node = $node->next
        set $idx = $idx + 1
    end
end

