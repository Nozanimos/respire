# FONCTION HELPER: Afficher un widget avec toutes ses infos
# â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
define show_widget
    set $node = $arg0
    set $idx = $arg1

    printf "[%2d] ", $idx

    if ($node->type == 0)
        printf "INCREMENT '%s' : x=%d, y=%d (base_x=%d, base_y=%d)\n", \
               $node->widget.increment_widget->option_name, \
               $node->widget.increment_widget->base.x, \
               $node->widget.increment_widget->base.y, \
               $node->widget.increment_widget->base.base_x, \
               $node->widget.increment_widget->base.base_y
    end

    if ($node->type == 1)
        printf "TOGGLE '%s' : x=%d, y=%d (base_x=%d, base_y=%d)\n", \
               $node->widget.toggle_widget->option_name, \
               $node->widget.toggle_widget->base.x, \
               $node->widget.toggle_widget->base.y, \
               $node->widget.toggle_widget->base.base_x, \
               $node->widget.toggle_widget->base.base_y
    end

    if ($node->type == 2)
        set $label = $node->widget.label_widget
        printf "LABEL '%s' : x=%d, y=%d (base_x=%d, base_y=%d) align=", \
               $label->text, $label->base.x, $label->base.y, \
               $label->base.base_x, $label->base.base_y
        if ($label->alignment == 0)
            printf "LEFT\n"
        else
            if ($label->alignment == 1)
                printf "CENTER\n"
            else
                printf "RIGHT\n"
            end
        end
    end

    if ($node->type == 3)
        printf "SEPARATOR : x=%d, y=%d (base_y=%d) width=%d (margins=%d/%d)\n", \
               $node->widget.separator_widget->base.x, \
               $node->widget.separator_widget->base.y, \
               $node->widget.separator_widget->base.base_y, \
               $node->widget.separator_widget->base.width, \
               $node->widget.separator_widget->base_start_margin, \
               $node->widget.separator_widget->base_end_margin
    end

    if ($node->type == 4)
        printf "PREVIEW : x=%d, y=%d (base_x=%d, base_y=%d) size=%d\n", \
               $node->widget.preview_widget->base.x, \
               $node->widget.preview_widget->base.y, \
               $node->widget.preview_widget->base.base_x, \
               $node->widget.preview_widget->base.base_y, \
               $node->widget.preview_widget->base_frame_size
    end

    if ($node->type == 5)
        printf "BUTTON '%s' : x=%d, y=%d (base_x=%d, base_y=%d)\n", \
               $node->widget.button_widget->text, \
               $node->widget.button_widget->base.x, \
               $node->widget.button_widget->base.y, \
               $node->widget.button_widget->base.base_x, \
               $node->widget.button_widget->base.base_y
    end

    if ($node->type == 7)
        printf "SELECTOR '%s' : x=%d, y=%d (base_x=%d, base_y=%d)\n", \
               $node->widget.selector_widget->nom_affichage, \
               $node->widget.selector_widget->base.x, \
               $node->widget.selector_widget->base.y, \
               $node->widget.selector_widget->base.base_x, \
               $node->widget.selector_widget->base.base_y
    end
end

